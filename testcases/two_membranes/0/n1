FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      n1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField  uniform 0.5;


boundaryField
{
    top
    {
        type fixedValue;
        value uniform 1.584947136820922;
        
    }


    bottom
    {
                type fixedValue;
        value uniform 1.584947136820922;
    }

    left
    {
          type            empty;
    }

    right
    {
        type            empty;
    }

    front
    {
        type            empty;    }

    back
    {
        type            empty;    }

}


