{
    top
    {
        type            fixedValue;
        value uniform -1.1536982351695373;
    }

    bottom
    {
        type            fixedValue;
        value uniform -1.1536982351695373;
    }

    left
    {
          type            empty;
    }

    right
    {
        type            empty;
    }

    front
    {
        type            empty;    }

    back
    {
        type            empty;    }

}


