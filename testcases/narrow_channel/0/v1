FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      v1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
            type zeroGradient;
    }

    bottom
    {
        type            zeroGradient;
    }
    left
    {
        type            zeroGradient;
    }

    right
    {
        type            zeroGradient;
    }

    front
    {
        type            empty;
    }

    back
    {
        type            empty;
    }

}


